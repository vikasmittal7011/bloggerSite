<%- include("partials/header") %>

    <h1>Compose</h1>

    <form action="/composeContent" method="post">
        <div class="form-group">
            <label class="fs-4">Title</label>
            <input type="text" name="postTitle" class="form-control">
        </div>
        <div class="form-group">
            <label class="fs-4">Post</label>
            <textarea type="text" name="postContent" class="form-control" rows="6"></textarea>
        </div>


        <button class="btn btn-primary" type="submit">Publish</button>
    </form>

    <%- include("partials/footer") %>